# BSC Mainnet Deployment Configuration
# Copy this file to .env and fill in your values

# Your private key for deployment (keep this secure!)
PRIVATE_KEY=your_private_key_here

# Treasury address to receive payments (optional, defaults to deployer address)
TREASURY_ADDRESS=your_treasury_address_here

# BscScan API key for contract verification (get from https://bscscan.com/apis)
BSC_API_KEY=your_bscscan_api_key_here

# Gas price in gwei (optional, adjust based on network conditions)
GAS_PRICE=3

# Enable gas reporting (optional)
REPORT_GAS=false

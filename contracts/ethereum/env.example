# Ethereum Mainnet Deployment Configuration
# Copy this file to .env and fill in your values

# Your private key for deployment (keep this secure!)
PRIVATE_KEY=your_private_key_here

# Treasury address to receive payments (optional, defaults to deployer address)
TREASURY_ADDRESS=your_treasury_address_here

# Infura Project ID (get from https://infura.io)
INFURA_PROJECT_ID=your_infura_project_id_here

# Alternative: Alchemy API Key (get from https://alchemy.com)
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Etherscan API key for contract verification (get from https://etherscan.io/apis)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Enable gas reporting (optional)
REPORT_GAS=false
